<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
<extension id="application" point="org.eclipse.core.runtime.applications">
	<application thread="any" cardinality="*" visible="true" >
		<run class="fr.obeo.ecore.fxdiag.app.MainApplication" />
		
	</application>
	
</extension>
<extension id="product" point="org.eclipse.core.runtime.products">
	<product application="fr.obeo.ecore.fxdiag.app.application" name="ecorefx" >
		<property name="appName" value="ecorefx" />
		
	</product>
	
</extension>
</plugin>
